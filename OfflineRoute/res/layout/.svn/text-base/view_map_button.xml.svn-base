<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:background="@drawable/my_iterenary_button1_state"
	android:onClick="ViewMapButtonClicked">
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="View Map"
		android:textColor="@color/theme_font_over_contrast"
		android:onClick="ViewMapButtonClicked"></TextView>
	<ImageView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:background="@drawable/viewmap_icon"
		android:layout_marginLeft="4dp" android:layout_marginRight="4dp"
		android:onClick="ViewMapButtonClicked"></ImageView>
</LinearLayout>
